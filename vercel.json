{
    "version": 2,
    "builds": [
      {
        "src": "public/js/index.js",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/public/js/index.js",
        "dest": "/public/js/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }
  